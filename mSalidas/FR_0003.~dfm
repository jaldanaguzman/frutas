  object qyVerificarFolio: TIBQuery
    Database = dmIBModulo.ibSistema
    Transaction = dmIBModulo.ibSistemaTransaccion
    SQL.Strings = (
      'SELECT *'
      'FROM VERIFICAR_FOLIO_COTIZACION(:P_ID, :P_FOLIO)')
    Left = 160
    Top = 16
    ParamData = <
      item
        DataType = ftString
        Name = 'P_ID'
        ParamType = ptUnknown
        Value = '0'
      end
      item
        DataType = ftString
